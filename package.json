{
  "name": "better-safe-typed-html",
  "version": "1.0.1",
  "description": "Type safe HTML JSX templates using TypeScript.",
  "main": "dist/elements.js",
  "typings": "dist/elements.d.ts",
  "module": "dist/elements.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mori-Enterprises/better-safe-typed-html"
  },
  "scripts": {
    "build": "tsc --project ./src/tsconfig.json",
    "build-test": "npm run build && tsc --project ./test/tsconfig.json",
    "pretest": "npm run build-test",
    "test": "mocha -r ts-node/esm --ui bdd './test/**/*.spec.{js,jsx,ts,tsx}'",
    "preversion": "npm test",
    "version": "npm run generate-changelog",
    "postversion": "npm publish && git push && git push --tags",
    "generate-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "start": "tsc -w -p .",
    "release:patch": "npm version patch -m \"chore(release): %s\"",
    "release:minor": "npm version minor -m \"chore(release): %s\"",
    "release:major": "npm version major -m \"chore(release): %s\""
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "typescript",
    "html",
    "typesafe",
    "template"
  ],
  "author": "Mori Nobuteru <nobu@mori.enterprises>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.21",
    "html-differ": "^1.4.0",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^10.9.1",
    "tslint": "^5.18.0",
    "typescript": "^4.1.2"
  }
}