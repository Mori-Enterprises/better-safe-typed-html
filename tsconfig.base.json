{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "esModuleInterop": true,
        "declaration": true,
        "composite": true,
        "declarationMap": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "jsx": "react",
        "jsxFactory": "elements.createElement",
        "lib": [
            "es2018",
            "dom"
        ],
        "types": []
    }
}